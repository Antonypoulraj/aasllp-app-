{
 "compilerOptions": {
   "baseUrl": "./src",
   
   "paths": {
    "@/*": ["*"]
    //"compilerOptions": {
 //     "jsx": "react-jsx",
  //  "moduleResolution": "node",
  //  "resolveJsonModule": true,
   //"esModuleInterop": true
  



    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"

  },

  "include": [
    "src"
  ]
}                                         